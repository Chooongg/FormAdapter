<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <!-- Activated -->
    <item android:color="?attr/colorPrimary" android:state_activated="true" />
    <!-- Disabled -->
    <item android:alpha="@dimen/m3_comp_outlined_text_field_disabled_supporting_text_opacity" android:color="@macro/m3_comp_outlined_text_field_disabled_supporting_text_color" android:state_enabled="false" tools:ignore="PrivateResource" />
    <!-- Focused -->
    <item android:color="@macro/m3_comp_outlined_text_field_focus_supporting_text_color" android:state_focused="true" />
    <!-- Hovered -->
    <item android:color="@macro/m3_comp_outlined_text_field_hover_supporting_text_color" android:state_hovered="true" />
    <!-- Normal -->
    <item android:color="@macro/m3_comp_outlined_text_field_supporting_text_color" />
</selector>